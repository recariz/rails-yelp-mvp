<h1>Create a new review for your restaurant</h1>
<div class="container">
  <div class="row">
    <div class="col">
      <%= simple_form_for [@restaurant, @review] do |f| %>
        <%= f.input :content %>
        <%= f.input :rating %>
        <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>
